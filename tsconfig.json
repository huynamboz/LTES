{
	"compilerOptions": {
	  "target": "ES2018",
	  "module": "ESNext",
	  "moduleResolution": "Node",
	  "lib": [
		"ESNext",
		"ESNext.AsyncIterable",
		"DOM"
	  ],
	  "esModuleInterop": true,
	  "sourceMap": true,
	  "experimentalDecorators": true,
	  "baseUrl": ".",
	  "paths": {
		"~/*": ["./*"],
		"@assets/*": ["./assets/*"],
	  },
	  "types": [
		"@nuxtjs/axios",
	  ]
	},
	"include": ["/**/*"],
	"exclude": [
	  "node_modules",
	  ".nuxt",
	  "dist"
	]
  }
  